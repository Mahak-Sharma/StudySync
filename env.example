# Environment Variables for StudySync

# Meeting Server URL (for group video calls)
VITE_MEETING_SERVER_URL=https://studysync-meeting.onrender.com

# Video Call Server URL (for 1:1 calls)
VITE_VIDEO_CALL_SERVER_URL=https://studysync-enqu.onrender.com

# Friends Backend URL
VITE_FRIENDS_BACKEND_URL=https://studysync-friends.onrender.com

# Summarization Backend URL
VITE_SUMMARIZATION_BACKEND_URL=https://studysync-summarization.onrender.com

# Firebase Configuration (if not using firebaseConfig.js)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id 